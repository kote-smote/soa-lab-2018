eureka:
  client:
    service-url:
      defaultZone: http://eureka:8761/eureka/
    healthcheck:
      enabled: false
  appInfo:
    replicate:
      interval: 3
    initial:
      replicate:
        time: 3
  instance:
    leaseRenewalIntervalInSeconds: 1
    leaseExpirationDurationInSeconds: 2

# Datasource properties
spring:
  datasource:
    url: jdbc:postgresql://l/soa-recruitment-db
    username: postgres
    password: postgres


spring.jpa:
  hibernate:
    ddl-auto: create
